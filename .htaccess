<FilesMatch "\.(js|css|html)$">
    Header unset Cache-Control
</FilesMatch>

Redirect /login /
Redirect /dashboard /
Redirect /files /
Redirect /settings /
Redirect /about /

RewriteCond %{REQUEST_METHOD} !=POST
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_URI} !(\.[a-zA-Z0-9]{1,5}|/)$
RewriteRule (.*)$ /$1/ [R=307,L]